{
    "name": "sola-php/sola-php",
    "description": "Headless Php Framework",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SolaPhp\\SolaPhp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Adham Saad",
            "email": "adham.sa3d@gmail.com"
        }
    ],
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "^10.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
